{
  "name": "deepiri",
  "version": "1.0.0",
  "description": "Deepiri - AI-Powered Task Management Platform",
  "private": true,
  "workspaces": [
    "services/*",
    "services/shared-utils",
    "api-server",
    "frontend"
  ],
  "scripts": {
    "install:all": "npm install",
    "install:services": "npm install --workspace=services/*",
    "install:shared": "npm install --workspace=services/shared-utils",
    "fix-deps": "bash scripts/fix-dependencies.sh"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/deepiri.git"
  },
  "keywords": [
    "ai",
    "task-management",
    "microservices",
    "nodejs",
    "python"
  ],
